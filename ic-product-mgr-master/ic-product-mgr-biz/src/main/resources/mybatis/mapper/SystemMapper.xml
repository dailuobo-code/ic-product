<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mallcai.bs.mapper.SystemMapper">

     <select id="getSysCode"  parameterType="string" resultType="SysCode">
		SELECT 
			*
		FROM 
			tbl_sys_code tsc
		WHERE 
			tsc.type=#{type}
	</select>
	
</mapper>